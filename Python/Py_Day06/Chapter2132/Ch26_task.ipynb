   ],
   "source": [
    "def odd_numbers(n=10):\n",
    "    nums = list()\n",
    "    for i in range(n):\n",
    "        if i % 2 != 0:\n",
    "            nums.append(i)\n",
    "    \n",
    "    return nums\n",
    "\n",
    "odd_numbers(20)\n"
   ]
  }
